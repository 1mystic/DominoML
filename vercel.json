{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "framework": "vite"
      }
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "cd app && npm run build",
  "outputDirectory": "app/dist"
}